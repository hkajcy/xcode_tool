[path]
LibPath		=/var/mobile/Applications/E92DD01C-5C72-4E8F-AFBE-6973CB5BAB27/Test.app//lib

[supporting language option]
MandarinSupport = 1;
CantoneseSupport = 1;
EnglishSupport = 1;


[lib option]
%for Mandarin
MandarinLibNum	= 2
MandarinLibName = [mandarin/xiaoyan.dat][mandarin/xiaotian.dat];
MandarinLibDesc = [小严(普通话男声)]   [小天(普通话女声)] 

%for Cantoniese
CantoneseLibNum  = 2
CantoneseLibName = [cantonese/xiaowang.dat] [cantonese/xiaowei.dat];
CantoneseLibDesc = [小王(粤语男声)]         [小韦(粤语女声)];

%for English
EnglishLibNum  = 2
EnglishLibName = [english/Jack.dat] [english/Rose.dat];
EnglishLibDesc = [Jack(英文男声)]   [Rose(英文女声)];

[loading option]
%LoadingType: 0:Mandarin; 1: Cantonese; 2:English
%LoadingIndex: for the lib to load
LoadMandarinIndex  = 0;
LoadCantoneseIndex = 0;
LoadEnglishIndex   = 0;

[running option]
%LangMode: 0:Mandarin; 1:Cantonese; 2:English; 3: MixLanguage
LangMode = 3;
MandarinAudLibIndex = 0
EnglishAudLibIndex = 0

